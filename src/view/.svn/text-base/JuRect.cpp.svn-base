#include "JuRect.h"

JuRect::JuRect(int x, int y, int w, int h) :
	m_x(x),
	m_y(y),
	m_w(w),
	m_h(h)
{

}

void JuRect::setX(int x) {
	m_x = x;
}

void JuRect::setY(int y) {
	m_y = y;
}

void JuRect::setWidth(int w) {
	m_w = w;
}

void JuRect::setHeight(int h) {
	m_h = h;
}

int JuRect::x() const {
	return m_x;
}

int JuRect::y() const {
	return m_y;
}

int JuRect::width() const {
	return m_w;
}

int JuRect::height() const {
	return m_h;
}
